lib main
use io

struct point {
	double x
	double y
}

struct entity {
	embed point
	point stuff[10]
}

void translate(point p, double x, double y) {
	p.x = p.x+x
}

int main() {
	entity en = {}
	translate(en,1,1)
	io:printf("%f", en.x)
	if (2 == 3) {
		io:printf("hi!")
	} else if 3 == 4 {
		io:printf("yo!")
	} else {
		io:printf("wtf!")
	}
	return 0
}