lib main
use io
use gui
use vec2

struct entity {
	embed vec2:xy
}

int main() {
	gui:start()

	window win = gui:createWindow("hi", 1000, 1000)

	pool entity en = { x = 1, y = 1 }

	while gui:tick() {
		printf("tick! %f", en.x)
		printf("tock! %f", en.y)
		vec2:add(en, 1, 1)
	}

	gui:destroyWindow(win)

	gui:stop()

	return 0
}